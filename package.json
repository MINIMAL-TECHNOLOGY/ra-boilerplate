{
  "name": "sems_studio_fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint $(git diff --name-only HEAD **/*.js, **/*.jsx, **/*.ts, **/*.tsx | xargs)",
    "lint:fix": "eslint --fix-dry-run $(git diff --name-only HEAD **/*.js, **/*.jsx, **/*.ts, **/*.tsx| xargs)",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier -- --write",
    "format": "yarn prettier:fix && yarn run lint:fix"
  },
  "dependencies": {
    "@mui/material": "^5.14.13",
    "@ra/infra": "https://github.com/phatnt199/ra-infra.git",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint-plugin-react": "^7.33.2",
    "react": "^18.2.0",
    "react-admin": "^4.15.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
