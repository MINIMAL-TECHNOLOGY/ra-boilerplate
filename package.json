{
  "name": "ra-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode dev-local",
    "build": "tsc && vite build",
    "lint": "eslint $(git diff --name-only HEAD **/*.js, **/*.jsx, **/*.ts, **/*.tsx | xargs)",
    "lint:fix": "eslint --fix-dry-run $(git diff --name-only HEAD **/*.js, **/*.jsx, **/*.ts, **/*.tsx| xargs)",
    "prepreview": "npm run build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier -- --write",
    "format": "yarn prettier:fix && yarn lint",
    "reconfig": "sh reconfig.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "@mui/x-date-pickers": "^6.16.3",
    "@reduxjs/toolkit": "^1.9.7",
    "browserslist-to-esbuild": "^1.2.0",
    "compressorjs": "^1.2.1",
    "dayjs": "^1.11.10",
    "eslint-plugin-react": "^7.33.2",
    "hangul-js": "^0.2.6",
    "lodash": "^4.17.21",
    "material-react-table": "^1.15.1",
    "ra-i18n-polyglot": "^4.15.1",
    "react": "^18.2.0",
    "react-admin": "^4.15.1",
    "react-daum-postcode": "^3.1.3",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "redux": "^4.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "main": "index.js",
  "repository": "https://github.com/MINIMAL-TECHNOLOGY/ra-boilerplate.git",
  "author": "TrNgTien <ngoctien27122001@gmail.com>",
  "license": "MIT"
}
